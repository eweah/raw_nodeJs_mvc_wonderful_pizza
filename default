    # ssl_ciphers ECDHE-RSA-AES256-GCM-SHA512:DHE-RSA-AES256-GCM-SHA512:ECDHE-RSA-AES256-GCM-SHA384:DHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-SHA384;
    # ssl_prefer_server_ciphers on;
    # ssl_dhparam /etc/nginx/dhparams.pem;

    add_header X-Frame-Options "SAMEORIGIN";
    add_header X-XSS-Protection "1; mode=block";
    add_header X-Content-Type-Options "nosniff";

    index index.html index.htm index.php;

    charset utf-8;
    gzip            on;
    gzip_types      text/plain application/xml text/css application/javascript;
    gzip_min_length 1000;

    # FORGE CONFIG (DO NOT REMOVE!)
    # include forge-conf/wonderfulpizza.devoutprogrammer.io/server/*;

    # location / {
    #     try_files $uri $uri/ /index.php?$query_string;
    # }
     location / {
        expires $expires;

        proxy_set_header Host               $host;
        proxy_set_header X-Real-IP          $remote_addr;
        proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto  $scheme;
        proxy_redirect              off;
        proxy_read_timeout          1m;
        proxy_connect_timeout       1m;
        proxy_pass                          http://127.0.0.1:3000; # set the address of the Node.js
    }


    location = /favicon.ico { access_log off; log_not_found off; }
    # location = /robots.txt  { access_log off; log_not_found off; }

    access_log off;
    # error_log  /var/log/nginx/wonderfulpizza.devoutprogrammer.io-error.log error;

    # error_page 404 /index.php;

    # location ~ \.php$ {
    #     fastcgi_split_path_info ^(.+\.php)(/.+)$;
    #     fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
    #     fastcgi_index index.php;
    #     include fastcgi_params;
    # }

    location ~ /\.(?!well-known).* {
        deny all;
    }

    listen [::]:443 ssl ; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/rawnodejs.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/rawnodejs.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot



}
server {
    if ($host = rawnodejs.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80 default_server;
	listen [::]:80 default_server;
    server_name rawnodejs.com;
    return 404; # managed by Certbot


}
server {
    if ($host = www.rawnodejs.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80 ;
	listen [::]:80 ;
    server_name www.rawnodejs.com;
    return 404; # managed by Certbot


}
